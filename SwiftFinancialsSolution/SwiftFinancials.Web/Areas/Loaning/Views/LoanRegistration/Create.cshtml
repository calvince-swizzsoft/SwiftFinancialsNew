            { "mDataProp": "FullName", "sTitle": "Name", "sWidth": "20%" },
            { "mDataProp": "Reference1", "sTitle": "Account Number", "sWidth": "15%" },
            { "mDataProp": "Reference2", "sTitle": "Membership  Number", "sWidth": "4%" },
