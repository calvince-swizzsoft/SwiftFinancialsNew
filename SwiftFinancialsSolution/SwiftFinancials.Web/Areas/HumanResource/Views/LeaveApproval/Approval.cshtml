    <script>

             //Station typehead starts here

            $('.LeaveTypeLookup').typeahead({
                source: function (query, process) {
                    return $.getJSON(
                        '@Url.Action("GetLeaveTypesAsync", "LeaveType", new { Area = "HumanResource" })',
                        { query: query },
                        function (LeaveTypes) {
                            var newLeaveTypesData = [];
                            LeaveTypesMap = {};
                            $.each(LeaveTypes, function (i, LeaveType) {
                                newLeaveTypesData.push(LeaveType.Description);
                                LeaveTypesMap[LeaveType.Description] = LeaveType;
                            });
                            process(newLeaveTypesData); return;
                        });
                },
                matcher: function (item) {
                    if (item.toLowerCase().indexOf(this.query.trim().toLowerCase()) != -1) {
                        return true;
                    }
                },
                sorter: function (items) {
                    return items.sort();
                },
                highlighter: function (item) {
                    var regex = new RegExp('(' + this.query + ')', 'gi');
                    return item.replace(regex, "<strong>$1</strong>");
                },
                updater: function (item) {
                    var LeaveTypeId = "";
                    LeaveTypeId = LeaveTypesMap[item].Id;
                    $('#LeaveTypeId').val(LeaveTypeId);
                    $('#LeaveTypeZoneDivisionEmployerDescription').val(LeaveTypesMap[item].ZoneDivisionEmployerDescription);
                    $('#LeaveTypeZoneDescription').val(LeaveTypesMap[item].ZoneDescription);
                    $('#LeaveTypeZoneDivisionDescription').val(LeaveTypesMap[item].ZoneDivisionDescription);
                    //console.log(LeaveTypeId)
                    return item;
                }
            });


        $('#EmployeesGrid').DataTable({
                // Ajax settings
                "sPaginationType": "full_numbers",
                "bProcessing": true,
                "bServerSide": true,
                "sAjaxSource": "@(Url.Action("Index", "Employee", new { Area = "HumanResource" }))",
