@model Application.MainBoundedContext.DTO.RegistryModule.CustomerBindingModel
@{


    ViewBag.Title = "Customer";
    var customerTypes = ViewBag.CustomerTypeSelectList as List<SelectListItem>;
    var individualTypes = ViewBag.IndividualTypeSelectList as List<SelectListItem>;
    var identityCardTypes = ViewBag.IdentityCardSelectList as List<SelectListItem>;
    var salutations = ViewBag.SalutationSelectList as List<SelectListItem>;
    var genders = ViewBag.GenderSelectList as List<SelectListItem>;
    var maritalStatuses = ViewBag.MaritalStatusSelectList as List<SelectListItem>;
    var nationalities = ViewBag.IndividualNationalitySelectList as List<SelectListItem>;
    var termsOfServices = ViewBag.IndividualEmploymentTermsOfServiceSelectList as List<SelectListItem>;
    var individualClassifications = ViewBag.IndividualClassificationSelectList as List<SelectListItem>;
}


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>Customer</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                @Html.ActionLink("Dashboard", "Index", "Home", new { Area = "" }, null)
            </li>
            <li class="breadcrumb-item">
                @Html.ActionLink("Home", "Index", "Customer", new { Area = "Registry" }, null)
            </li>
            <li class="breadcrumb-item active">
                <strong>Create</strong>
            </li>
        </ol>
    </div>
</div>

@if (TempData["Error"] != null)
{
    <div class="alert alert-danger fade in">
        <button class="close" data-dismiss="alert">
        </button>
        <i class="fa-fw fa fa-times"></i>
        <strong>Error!</strong> @TempData["Error"]
    </div>
}


<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">

                    <h2>Customers</h2>

                    @if (TempData["Error"] != null)
                    {
                        <div class="alert alert-danger">
                            <strong>Customer Registration Failed !</strong> @TempData["Error"]
                        </div>
                    }

                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content bg-white text-info">
                    @using (Html.BeginForm("Create", "Customer", new { Area = "Registry" }, FormMethod.Post, new { @class = "wizard-big", @id = "form" }))
                    {
                        @Html.AntiForgeryToken()

                        <div class="row">
                            <div class="col-4">
                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.TypeDescription, htmlAttributes: new { @class = "" })
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                        @Html.DropDownListFor(model => model.TypeDescription, customerTypes, new { @class = "form-control input-md" })
                                        @Html.ValidationMessageFor(model => model.TypeDescription)
                                    </div>
                                </div>

                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.StationId, htmlAttributes: new { @class = "control-label" })
                                    @Html.HiddenFor(model => model.StationId)
                                    @Html.EditorFor(model => model.StationDescription, new { htmlAttributes = new { @class = "StationsLookup form-control required", placeholder = "Description.." } })
                                </div>

                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.Reference1, htmlAttributes: new { @class = "" })
                                    @Html.EditorFor(model => model.Reference1, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.Reference1, "", new { @class = "text-danger" })
                                </div>

                            </div>

                            <div class="col-4">


                                <div class="form-group" id="data_3">
                                    @Html.LabelFor(model => model.RegistrationDate, htmlAttributes: new { @class = "" })
