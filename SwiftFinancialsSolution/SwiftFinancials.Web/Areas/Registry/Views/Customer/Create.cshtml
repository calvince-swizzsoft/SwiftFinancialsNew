                                    @Html.ValidationMessageFor(model => model.IndividualPayrollNumbers, "", new { @class = "text-danger" })
                                </div>

                                <div class="form-group form-group-lg">

                                    @Html.LabelFor(model => model.StationZoneDivisionDescription, htmlAttributes: new { @class = "" })
                                    @Html.EditorFor(model => model.StationZoneDivisionDescription, new { htmlAttributes = new { @class = "form-control", @placeholder = "Division", disabled = "disabled" } })
                                    @Html.ValidationMessageFor(model => model.StationZoneDivisionDescription, "", new { @class = "text-danger" })
                                </div>


                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.Reference2, htmlAttributes: new { @class = "" })
                                    @Html.EditorFor(model => model.Reference2, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.Reference2, "", new { @class = "text-danger" })
                                </div>

                            </div>
                        </div>

                        <br />


