                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualType, htmlAttributes: new { @class = "" })
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                                @Html.DropDownListFor(model => model.IndividualType, individualTypes, new { @class = "form-control input-md" })
                                                @Html.ValidationMessageFor(model => model.IndividualType)
                                            </div>
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualEmploymentDesignation, htmlAttributes: new { @class = "" })
                                            @Html.EditorFor(model => model.IndividualEmploymentDesignation, new { htmlAttributes = new { @class = "form-control", @placeholder = "Individual Employment Designation" } })
                                            @Html.ValidationMessageFor(model => model.IndividualEmploymentDesignation, "", new { @class = "text-danger" })
                                        </div>
                                    </div>

                                    <div class="col-4">
                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualGender, htmlAttributes: new { @class = "" })
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                                @Html.DropDownListFor(model => model.IndividualGender, genders, new { @class = "form-control input-md" })
                                                @Html.ValidationMessageFor(model => model.IndividualGender)
                                            </div>
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualFirstName, htmlAttributes: new { @class = "" })
                                            @Html.EditorFor(model => model.IndividualFirstName, new { htmlAttributes = new { @class = "form-control", @placeholder = "First Name" } })
                                            @Html.ValidationMessageFor(model => model.IndividualFirstName, "", new { @class = "text-danger" })
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.PersonalIdentificationNumber, htmlAttributes: new { @class = "" })
                                            @Html.EditorFor(model => model.PersonalIdentificationNumber, new { htmlAttributes = new { @class = "form-control", @placeholder = "PIN" } })
                                            @Html.ValidationMessageFor(model => model.PersonalIdentificationNumber, "", new { @class = "text-danger" })
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualMaritalStatus, htmlAttributes: new { @class = "" })
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                                @Html.DropDownListFor(model => model.IndividualMaritalStatus, maritalStatuses, new { @class = "form-control input-md" })
                                                @Html.ValidationMessageFor(model => model.IndividualMaritalStatus)
                                            </div>
                                        </div>

                                        <div class="form-group" id="data_3">
                                            @Html.LabelFor(model => model.IndividualEmploymentDate, htmlAttributes: new { @class = "" })
                                            <div class="input-group date">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                <input type="text" name="employmentdate" class="form-control" value="@DateTime.Today.AddDays(-30).ToString("dd/MM/yyyy")">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-4">
                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualIdentityCardType, htmlAttributes: new { @class = "" })
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                                @Html.DropDownListFor(model => model.IndividualIdentityCardType, identityCardTypes, new { @class = "form-control input-md" })
                                                @Html.ValidationMessageFor(model => model.IndividualIdentityCardType)
                                            </div>
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualLastName, htmlAttributes: new { @class = "" })
                                            @Html.EditorFor(model => model.IndividualLastName, new { htmlAttributes = new { @class = "form-control", @placeholder = "Last Name", @required = "" } })
                                            @Html.ValidationMessageFor(model => model.IndividualLastName, "", new { @class = "text-danger" })
                                        </div>

                                        <div class="form-group" id="data_3">
                                            @Html.LabelFor(model => model.IndividualBirthDate, htmlAttributes: new { @class = "" })
                                            <div class="input-group date">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                                <input type="text" name="birthdate" class="form-control" value="@DateTime.Today.AddDays(-30).ToString("dd/MM/yyyy")">
                                            </div>
                                        </div>

                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.IndividualEmploymentTermsOfService, htmlAttributes: new { @class = "" })
                                            <div class="input-group">
