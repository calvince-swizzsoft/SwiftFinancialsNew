    <script>
        $(document).ready(function () {



            $('#data_3 .input-group.date').datepicker({
                startView: 2,
                todayBtn: "linked",
                keyboardNavigation: false,
                forceParse: false,
                autoclose: true,
                format: "dd/mm/yyyy"  // Set the date format
            });

            //roles typehead starts here
            $('.rolesLookup').typeahead({
                source: function (query, process) {
                    return $.getJSON(
                        '@Url.Action("GetRoles", "Role", new { Area = "Admin" })',
                        { query: query },
                        function (roles) {
                            var newRolesData = [];
                            rolesMap = {};
                            $.each(roles, function (i,role) {
                                newRolesData.push(role.Name);
                                rolesMap[role.Name] = role;
                            });
                            process(newRolesData); return;
                        });
                },
                matcher: function (item) {
                    if (item.toLowerCase().indexOf(this.query.trim().toLowerCase()) != -1) {
                        return true;
                    }
                },
                sorter: function (items) {
                    return items.sort();
                },
                highlighter: function (item) {
                    var regex = new RegExp('(' + this.query + ')', 'gi');
                    return item.replace(regex, "<strong>$1</strong>");
                },
                updater: function (item) {
                    var roleId = "";
                    roleId = rolesMap[item].Id;
                    $('#RoleId').val(roleId);
                    //console.log(roleId)
                    return item;
                }
            });

            //Station typehead starts here
            $('.StationsLookup').typeahead({
                source: function (query, process) {
                    return $.getJSON(
                        '@Url.Action("GetStationsAsync", "Station", new { Area = "Registry" })',
                        { query: query },
                        function (Stations) {
