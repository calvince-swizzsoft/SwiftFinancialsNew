                                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                        <input type="text" name="registrationdate" class="form-control" value="@DateTime.Today.AddDays(-30).ToString("dd/MM/yyyy")">
                                    </div>
                                </div>

                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.StationZoneDescription, htmlAttributes: new { @class = "" })
                                    @Html.EditorFor(model => model.StationZoneDescription, new { htmlAttributes = new { @class = "form-control", @placeholder = "Zone", disabled = "disabled" } })
                                    @Html.ValidationMessageFor(model => model.StationZoneDescription, "", new { @class = "text-danger" })
                                </div>

                                <div class="form-group form-group-lg">
                                    @Html.LabelFor(model => model.Reference3, htmlAttributes: new { @class = "" })
                                    @Html.EditorFor(model => model.Reference3, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.Reference3, "", new { @class = "text-danger" })
                                </div>


                            </div>

                            <div class="col-4">
