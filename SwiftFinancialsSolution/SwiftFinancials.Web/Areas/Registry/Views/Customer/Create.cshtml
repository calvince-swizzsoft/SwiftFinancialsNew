                                                <div class="form-group form-group-lg">
                                                    @Html.LabelFor(model => model.AddressMobileLine, htmlAttributes: new { @class = "" })
                                                    @Html.EditorFor(model => model.AddressMobileLine, new { htmlAttributes = new { @class = "form-control", @placeholder = "Mobile line" } })
                                                    @Html.ValidationMessageFor(model => model.AddressMobileLine, "", new { @class = "text-danger" })
                                                </div>
                                                <div class="form-group form-group-lg">
                                                    @Html.LabelFor(model => model.AddressLandLine, htmlAttributes: new { @class = "" })
                                                    @Html.EditorFor(model => model.AddressLandLine, new { htmlAttributes = new { @class = "form-control", @placeholder = "Land line" } })
                                                    @Html.ValidationMessageFor(model => model.AddressLandLine, "", new { @class = "text-danger" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="form-group form-group-lg">
                                                    @Html.LabelFor(model => model.AddressStreet, htmlAttributes: new { @class = "" })
