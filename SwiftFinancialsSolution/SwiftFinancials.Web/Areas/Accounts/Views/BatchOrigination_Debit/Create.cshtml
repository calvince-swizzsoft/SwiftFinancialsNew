<script>


    $(document).ready(function () {
            $('.remove-btn').click(function () {
                var row = $(this).closest('tr');
                var id = row.data('id');


                $.ajax({
                    url: '@Url.Action("removeChargeSplit", "BatchOrigination_Voucher")',
                    type: 'POST',
                    data: { id: id },
                    success: function () {
                        row.remove();
                    },
                    error: function () {
                        alert('Error removing row.');
                    }
                });
            });
     });


    function handleSearchClick() {
        // Store form data in SessionStorage
        let formData = {
            Reference: document.querySelector('[name="Reference"]').value,
            Priority: document.querySelector('[name="Priority"]').value,
            BranchDescription: document.querySelector('[name="BranchDescription"]').value,
            BranchId: document.querySelector('[name="BranchId"]').value,
            DebitTypeDescription: document.querySelector('[name="DebitTypeDescription"]').value,
            DebitTypeId: document.querySelector('[name="DebitTypeId"]').value,
        };

        // Save the form data to SessionStorage
        sessionStorage.setItem('formData', JSON.stringify(formData));

        // Send the form data to the server
        $.ajax({
            type: 'POST',
            url: '/BatchOrigination_Debit/BatchOrigination_Debit',
            data: JSON.stringify(formData),
            contentType: 'application/json',
            headers: {
                'RequestVerificationToken': document.querySelector('[name="__RequestVerificationToken"]').value
            },
            success: function (response) {
                // Trigger the modal after saving
                $('#DebitcustomerAccountLookupModal').modal('show');
            },
            error: function (error) {
                console.error('Error saving form data:', error);
                // Handle the error if needed
            }
        });
    }

     $('#BenefactorCustomerAccountsGrid').DataTable({
       // Ajax settings
        "sPaginationType": "full_numbers",
        "bProcessing": true,
        "bServerSide": true,
        "sAjaxSource": "@(Url.Action("Index", "DebitType", new { Area = "Accounts"}))",
        "sServerMethod": "POST",
        // Callback settings
        "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
            var createdDate = new Date(parseInt(aData.CreatedDate.replace("/Date(", "").replace(")/", ""), 10));
            var h = ("0" + createdDate.getHours()).slice(-2);
            var m = ("0" + createdDate.getMinutes()).slice(-2);
            var s = ("0" + createdDate.getSeconds()).slice(-2);

            var dt_Created = $.datepicker.formatDate('dd/mm/yy', createdDate) + " " + h + ":" + m + ":" + s;
            aData.CreatedDate = dt_Created;

            // var selectButton = '<button type="button" data-id="' + aData.Id + '" class="btn btn-outline-info select-btn">Select</button>';
            var selectButton = '<a href="/Accounts/BatchOrigination_Debit/Create/' + aData.Id + '" class="btn btn-outline-info select-link">' + 'Select</a>';

            // Assuming you are using jQuery
            $(document).ready(function () {
                // Attach click event handler to the link
                $('.select-link').click(function (event) {
                    // Prevent default action (page reload)
                    event.preventDefault();
                    // You can add additional logic here if needed

                    // Optionally, if you want to navigate programmatically after preventing default action:
                    window.location.href = $(this).attr('href');
                });
            });

            $('td:eq(4)', nRow).html(dt_Created);
            $('td:eq(5)', nRow).html(selectButton);

            return nRow;
        },
         // Display settings
         "aoColumns": [
             { "mDataProp": "Description", "sTitle": "Name", "sWidth": "15%" },
             { "mDataProp": "CustomerAccountTypeProductCodeDescription", "sTitle": "Product Code", "sWidth": "10%" },
             { "mDataProp": "CustomerAccountTypeTargetProductDescription", "sTitle": "Product Name", "sWidth": "15%" },
             { "mDataProp": "IsLocked", "sTitle": "IsLocked", "sWidth": "5%" },
             { "mDataProp": "CreatedDate", "sTitle": "Created Date", "sWidth": "10%" },
             { "mDataProp": "CreatedDate", "sTitle": "Action", "sWidth": "6%", "bSortable": false }],
         "aaSorting": [[0, "desc"]]
    });

     //DebitType typehead starts here
            $('.DebitTypesLookup').typeahead({
                source: function (query, process) {
                    return $.getJSON(
                        '@Url.Action("GetDebitTypesAsync", "BatchOrigination_Debit", new { Area = "Accounts" })',
                        { query: query },
                        function (DebitTypes) {
                            var newDebitTypesData = [];
                            DebitTypesMap = {};
                            $.each(DebitTypes, function (i, DebitType) {
                                newDebitTypesData.push(DebitType.Description);
                                DebitTypesMap[DebitType.Description] = DebitType;
                            });
                            process(newDebitTypesData); return;
                        });
                },
                matcher: function (item) {
                    if (item.toLowerCase().indexOf(this.query.trim().toLowerCase()) != -1) {
                        return true;
                    }
                },
                sorter: function (items) {
                    return items.sort();
                },
                highlighter: function (item) {
                    var regex = new RegExp('(' + this.query + ')', 'gi');
                    return item.replace(regex, "<strong>$1</strong>");
                },
                updater: function (item) {
                    var debitTypeId = "";
                    DebitTypeId = DebitTypesMap[item].Id;
                    $('#DebitTypeId').val(DebitTypeId);
                    //console.log(DebitTypeId)
                    return item;
                }
            });





                 //Branch typehead starts here
            $('.BranchesLookup').typeahead({
                source: function (query, process) {
                    return $.getJSON(
                        '@Url.Action("GetBranchesAsync", "Branch", new { Area = "Admin" })',
                        { query: query },
                        function (Branches) {
                            var newBranchesData = [];
                            BranchesMap = {};
                            $.each(Branches, function (i, Branch) {
                                newBranchesData.push(Branch.Description);
                                BranchesMap[Branch.Description] = Branch;
                            });
                            process(newBranchesData); return;
                        });
                },
                matcher: function (item) {
                    if (item.toLowerCase().indexOf(this.query.trim().toLowerCase()) != -1) {
                        return true;
                    }
                },
                sorter: function (items) {
                    return items.sort();
                },
                highlighter: function (item) {
                    var regex = new RegExp('(' + this.query + ')', 'gi');
                    return item.replace(regex, "<strong>$1</strong>");
                },
                updater: function (item) {
                    var branchId = "";
                    BranchId = BranchesMap[item].Id;
                    $('#BranchId').val(BranchId);
                    //console.log(BranchId)
                    return item;
                }

            });

