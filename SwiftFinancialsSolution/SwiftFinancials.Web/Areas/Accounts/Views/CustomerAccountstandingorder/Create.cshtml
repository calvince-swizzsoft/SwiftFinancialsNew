        function selectBeneficiaryCustomerAccounts(id) {
            $.ajax({
                url: '/Accounts/CustomerAccountstandingorder/BeneficiaryCustomerAccountLookUp',
                type: 'POST',
                data: { Id: id },
                success: function (response) {
                    if (response.success) {
                        $('#BeneficiaryCustomerAccountId').val(response.data.BeneficiaryCustomerAccountId);
                        $('#BeneficiaryFullAccountNumber').val(response.data.BeneficiaryFullAccountNumber);
                        $('#BeneficiaryCustomerFullName').val(response.data.BeneficiaryCustomerAccountCustomerFullName);
                        $('#BeneficiaryProductDescription').val(response.data.BeneficiaryProductDescription);

                        $('#BeneficiarycustomerLookupModal').modal('hide');
                    } else {
                        alert(response.message);
                    }
                },
                error: function () {
                    alert('An error occurred while processing your request.');
                }
            });
        }
    </script>

