    <script>
    $(document).ready(function () {
        var selectedLoans = [];

        // Function to save checkbox state to local storage
        function saveCheckboxState(key, state) {
            localStorage.setItem(key, JSON.stringify(state));
        }

        // Function to load checkbox state from local storage
        function loadCheckboxState(key) {
            return JSON.parse(localStorage.getItem(key)) || false;
        }

        // Function to update the selected loans array
        function updateSelectedLoans() {
            $('.lrowCheckbox').each(function () {
                var loanId = $(this).data('id');
                if ($(this).prop('checked')) {
                    if (!selectedLoans.includes(loanId)) {
                        selectedLoans.push(loanId);
                    }
                } else {
                    selectedLoans = selectedLoans.filter(id => id !== loanId);
                }
            });
        }

        // Check or uncheck "Select All" checkbox based on current page selection state
        function updateSelectAllCheckbox() {
            var allChecked = $('.lrowCheckbox:checked').length === $('.lrowCheckbox').length && $('.lrowCheckbox').length > 0;
            $('#lheaderCheckbox').prop('checked', allChecked);
            saveCheckboxState('lheaderCheckbox', allChecked);
        }

        // Initialize DataTable
        var table = $('#LoanProductsGrid').DataTable({
            "sPaginationType": "full_numbers",
            "bProcessing": true,
            "bServerSide": true,
            "sAjaxSource": "@(Url.Action("Index", "LoanProduct", new { Area = "Accounts"}))",
            "sServerMethod": "POST",

            // Callback for each row to inject the checkbox and retain its state
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
                var checkboxHTML = '<input type="checkbox" class="lrowCheckbox" data-id="' + aData.Id + '"';

                // If the loan is already selected, check the checkbox
                if (selectedLoans.includes(aData.Id)) {
                    checkboxHTML += ' checked';
                }

                checkboxHTML += '>';
                $('td:eq(0)', nRow).html(checkboxHTML);
                return nRow;
            },

            // Define columns for DataTable
            "aoColumns": [
                { "mDataProp": "Id", "sTitle": "Select", "sWidth": "0.09%" },
                { "mDataProp": "Description", "sTitle": "Name", "sWidth": "10%", "bSortable": false }
            ],
            "aaSorting": [[0, "desc"]],

            // Ensure checkboxes are retained across pages
            "fnDrawCallback": function () {
                // Restore checkbox state after table redraw
                updateSelectAllCheckbox();
            }
        });

        // Event listener for the "Select All" checkbox
        $('#lheaderCheckbox').on('change', function () {
            var isChecked = $(this).prop('checked');
            $('.lrowCheckbox').prop('checked', isChecked);

            // If "Select All" is checked, add all loans from the current page to the selectedLoans array
            if (isChecked) {
                table.rows({ page: 'current' }).every(function () {
                    var loanId = this.data().Id;
                    if (!selectedLoans.includes(loanId)) {
                        selectedLoans.push(loanId);
                    }
                });
            } else {
                // Remove loans from the current page from the selectedLoans array
                table.rows({ page: 'current' }).every(function () {
                    var loanId = this.data().Id;
                    selectedLoans = selectedLoans.filter(id => id !== loanId);
                });
            }

            saveCheckboxState('lheaderCheckbox', isChecked);
        });

        // Event listener for individual checkboxes
        $('#LoanProductsGrid').on('change', '.lrowCheckbox', function () {
            updateSelectedLoans();
            updateSelectAllCheckbox();
        });

        // Add the "Select All" checkbox to the grid
        $('#LoanProductsGrid_wrapper .dataTables_scroll').prepend('<div><input type="checkbox" id="lheaderCheckbox"> Select / Deselect All</div>');

        // Restore the state of the header checkbox after page load
        $('#lheaderCheckbox').prop('checked', loadCheckboxState('lheaderCheckbox'));

        // Submit button for sending selected loans to the server
        $('#submitButton').on('click', function () {
            $.ajax({
                url: '@Url.Action("ProcessSelectedLoans", "LoanProduct", new { Area = "Accounts" })',
                type: 'POST',
                data: { selectedLoanIds: selectedLoans },
                success: function (response) {
                    alert('Loans processed successfully.');
                },
                error: function (error) {
                    alert('An error occurred.');
                }
            });
        });
    });
    </script>


