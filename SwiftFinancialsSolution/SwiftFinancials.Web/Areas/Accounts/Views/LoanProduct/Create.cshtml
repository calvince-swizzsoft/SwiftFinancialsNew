        // Implementation for saving checkbox state goes here
    }

    // Function to load the state of the header checkbox from local storage
    function loadCheckboxState(key) {
        // Implementation for loading checkbox state goes here
    }

    $(document).ready(function () {
        // Event listener for header checkbox
        $('#sheaderCheckbox').on('change', function () {
            // Check or uncheck all row checkboxes based on the header checkbox state
            $('.srowCheckbox').prop('checked', $(this).prop('checked')).each(function () {
                var checkboxKey = $(this).attr('id');
                // Save the state of the checkbox to local storage
                saveCheckboxState(checkboxKey, $(this).prop('checked'));
            });
            // Save the state of the header checkbox to local storage
            saveCheckboxState('sheaderCheckbox', $(this).prop('checked'));
        });

        // Event listener for individual row checkboxes
        $('#IndefiniteChargesGrid').on('change', '.srowCheckbox', function () {
            // Save the state of the checkbox to local storage
            var checkboxKey = $(this).attr('id');
            saveCheckboxState(checkboxKey, $(this).prop('checked'));

            // Uncheck header checkbox if any row checkbox is unchecked
            if (!$(this).prop('checked')) {
                $('#sheaderCheckbox').prop('checked', false);
                // Save the state of the header checkbox to local storage
                saveCheckboxState('sheaderCheckbox', false);
            } else {
                // Check if all row checkboxes are checked and check the header checkbox accordingly
                var allChecked = $('.srowCheckbox:checked').length === $('.srowCheckbox').length;
                $('#sheaderCheckbox').prop('checked', allChecked);
                // Save the state of the header checkbox to local storage
                saveCheckboxState('sheaderCheckbox', allChecked);
            }
        });

        // Load the state of the header checkbox from local storage after the page is loaded
        $('#sheaderCheckbox').prop('checked', loadCheckboxState('sheaderCheckbox'));
