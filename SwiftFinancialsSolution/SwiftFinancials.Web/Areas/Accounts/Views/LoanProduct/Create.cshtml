            const addButton = document.getElementById('addEntryBtn2');

            // Add click event listener to the button
            addButton.addEventListener('click', submitDeductiblesForm);
        });

        function submitDeductiblesForm() {
            // Get form data
            const description = document.querySelector('[name="Deductiles[0].Description"]').value;
            const productCode = document.querySelector('[name="Deductiles[0].CustomerAccountTypeProductCodeDescription"]').value;
            const productName = document.querySelector('[name="Deductiles[0].CustomerAccountTypeTargetProductDescription"]').value;
            const chargeType = document.querySelector('[name="Deductiles[0].ChargeTypeDescription"]').value;
            const chargePercentage = document.querySelector('[name="Deductiles[0].ChargePercentage"]').value;

            // Create an object with the form data
            const formData = {
                Description: description,
                CustomerAccountTypeProductCodeDescription: productCode,
                CustomerAccountTypeTargetProductDescription: productName,
                ChargeTypeDescription: chargeType,
                ChargePercentage: chargePercentage
            };

            // Send AJAX request
            fetch('/LoanProduct/AddDeductibles', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                    'X-CSRF-TOKEN': document.querySelector('input[name="__RequestVerificationToken"]').value
                },
                body: JSON.stringify(formData)
            })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        // Add the new row to the table
                        addRowToTable(data.deductible);
                        // Clear the form fields
                        clearFormFields();
                    } else {
                        alert('Error: ' + data.message);
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    alert('An error occurred while submitting the form');
                });
        }

