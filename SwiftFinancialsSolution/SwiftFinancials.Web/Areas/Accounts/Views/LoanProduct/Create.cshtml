<script>
    $(document).ready(function () {
    var selectedCharges = [];

    // Load checkbox state from localStorage on page load
    function loadCheckboxState() {
        var storedCharges = localStorage.getItem('selectedCharges');
        if (storedCharges) {
            selectedCharges = JSON.parse(storedCharges);

            // Restore checkbox states
            selectedCharges.forEach(function (charge) {
                var checkbox = $('#ic_' + charge.Id);
                if (checkbox.length) {
                    checkbox.prop('checked', true);
                }
            });
        }
    }

    // Save the selected charges in localStorage
    function saveCheckboxState() {
        localStorage.setItem('selectedCharges', JSON.stringify(selectedCharges));
    }

    // Initialize the DataTable
    var table = $('#IndefiniteChargesGrid').DataTable({
        "sPaginationType": "full_numbers",
        "bProcessing": true,
        "bServerSide": true,
        "sAjaxSource": "@(Url.Action("Index", "IndefiniteCharges", new { Area = "Accounts" }))",
        "sServerMethod": "POST",
        "fnRowCallback": function (nRow, aData) {
            var checkboxHTML = '<input type="checkbox" class="icrowCheckbox" id="ic_' + aData.Id + '"';
            if (aData.AutomateLedgerFeeCalculation) {
                checkboxHTML += ' checked';
            }
            checkboxHTML += '>';
            $('td:eq(0)', nRow).html(checkboxHTML);

            // If charge was previously selected, check it
            var existingCharge = selectedCharges.find(function (charge) {
                return charge.Id === aData.Id.toString();
            });
            if (existingCharge) {
                $('#ic_' + aData.Id).prop('checked', true);
            }

            return nRow;
        },
        "aoColumns": [
            { "mDataProp": "Id", "sTitle": "Select", "sWidth": "0.1%" },
            { "mDataProp": "Description", "sTitle": "Name", "sWidth": "10%", "bSortable": false }
        ],
        "aaSorting": [[1, "asc"]],
        "initComplete": function () {
            loadCheckboxState();  // Load checkbox state on pagination
        }
    });

    // Handle individual row checkbox change
    $('#IndefiniteChargesGrid').on('change', '.icrowCheckbox', function () {
        var chargeId = $(this).attr('id').replace('ic_', '');
        var chargeDescription = $(this).closest('tr').find('td:eq(1)').text();  // Assuming description is in the second column

        if ($(this).is(':checked')) {
            selectedCharges.push({
                Id: chargeId,
                Description: chargeDescription
            });  // Add to array
        } else {
            // Remove unselected charge
            selectedCharges = selectedCharges.filter(function (charge) {
                return charge.Id !== chargeId;
            });
        }

        // Save checkbox state in localStorage
        saveCheckboxState();
    });

    // On form submit, send the selected charges to the controller
    $('form').on('submit', function (e) {
        e.preventDefault(); // Prevent the default form submission

        $.ajax({
            type: 'POST',
            url: '@Url.Action("SelectedCharges", "LoanProduct")', // Replace with your controller name
            contentType: 'application/json',
            data: JSON.stringify(selectedCharges), // Send the selected charges directly
            success: function (response) {
                if (response.success) {
                    console.log("Charges submitted successfully");
                    // Optionally, handle success (e.g., show a message or redirect)
                } else {
                    console.log("Error submitting charges");
                }
            },
            error: function (error) {
                console.log("Error occurred while submitting charges: ", error);
            }
        });
    });

    // Add header checkbox for select/deselect all
    $('#IndefiniteChargesGrid_wrapper .dataTables_scroll').prepend('<div><input type="checkbox" id="icheaderCheckbox"> Select / Deselect All</div>');

    // Handle header checkbox change
    $('#icheaderCheckbox').on('change', function () {
        $('.icrowCheckbox').prop('checked', $(this).prop('checked')).each(function () {
            var chargeId = $(this).attr('id').replace('ic_', '');
            var chargeDescription = $(this).closest('tr').find('td:eq(1)').text();

            if ($(this).prop('checked')) {
                selectedCharges.push({
                    Id: chargeId,
                    Description: chargeDescription
                });
            } else {
                selectedCharges = selectedCharges.filter(function (charge) {
                    return charge.Id !== chargeId;
                });
            }
        });

        // Save checkbox state in localStorage
        saveCheckboxState();
    });

    // On page load, restore the hidden field value
    var storedCharges = localStorage.getItem('selectedCharges');
    if (storedCharges) {
        selectedCharges = JSON.parse(storedCharges);
    }
});



</script>








    <script>
        //Savingd Products
    $(document).ready(function () {
        var selectedProducts = [];

        // Function to update the selected products array
        function updateSelectedProducts() {
            $('.srowCheckbox').each(function () {
                var productId = $(this).data('id');
                if ($(this).prop('checked')) {
                    if (!selectedProducts.includes(productId)) {
                        selectedProducts.push(productId);
                    }
                } else {
                    selectedProducts = selectedProducts.filter(id => id !== productId);
                }
            });
        }

        // Check or uncheck "Select All" checkbox based on the current page selection state
        function updateSelectAllCheckbox() {
            var allChecked = $('.srowCheckbox:checked').length === $('.srowCheckbox').length && $('.srowCheckbox').length > 0;
            $('#sheaderCheckbox').prop('checked', allChecked);
        }

        // Event listener for the "Select All" checkbox
        $('#sheaderCheckbox').on('change', function () {
            var isChecked = $(this).prop('checked');
            $('.srowCheckbox').prop('checked', isChecked);

            // If Select All is checked, add all products from the current page to the selectedProducts array
            if (isChecked) {
                table.rows({ page: 'current' }).every(function () {
                    var productId = this.data().Id;
                    if (!selectedProducts.includes(productId)) {
                        selectedProducts.push(productId);
                    }
                });
            } else {
                // Remove products from the current page from the selectedProducts array
                table.rows({ page: 'current' }).every(function () {
                    var productId = this.data().Id;
                    selectedProducts = selectedProducts.filter(id => id !== productId);
                });
            }
        });

        // Event listener for individual checkboxes
        $('#SavingsProductGrid').on('change', '.srowCheckbox', function () {
            updateSelectedProducts();
            updateSelectAllCheckbox();
        });

        // Initialize DataTable
        var table = $('#SavingsProductGrid').DataTable({
            "sPaginationType": "full_numbers",
            "bProcessing": true,
            "bServerSide": true,
            "sAjaxSource": "@(Url.Action("Index", "SavingsProduct", new { Area = "Accounts" }))",
            "sServerMethod": "POST",

            // Callback for each row to inject the checkbox and retain its state
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
                var checkboxHTML = '<input type="checkbox" class="srowCheckbox" data-id="' + aData.Id + '"';

                // If the product is already selected, check the checkbox
                if (selectedProducts.includes(aData.Id)) {
                    checkboxHTML += ' checked';
                }

                checkboxHTML += '>';
                $('td:eq(0)', nRow).html(checkboxHTML);
                return nRow;
            },

            // Define columns for DataTable
            "aoColumns": [
                { "mDataProp": "Id", "sTitle": "Select", "sWidth": "0.1%" },
                { "mDataProp": "Description", "sTitle": "Name", "sWidth": "10%", "bSortable": false }
            ],
            "aaSorting": [[0, "desc"]],

            // Ensure checkboxes are retained across pages
            "fnDrawCallback": function () {
                // Update the state of checkboxes after page navigation
                updateSelectAllCheckbox();
            }
        });

        // Submit button for sending selected products to the server
        $('#submitButton').on('click', function () {
            $.ajax({
                url: '@Url.Action("ProcessSelectedProducts", "SavingsProduct", new { Area = "Accounts" })',
                type: 'POST',
                data: { selectedProductIds: selectedProducts },
                success: function (response) {
                    alert('Products processed successfully.');
                },
                error: function (error) {
                    alert('An error occurred.');
                }
            });
        });
    });
    </script>


