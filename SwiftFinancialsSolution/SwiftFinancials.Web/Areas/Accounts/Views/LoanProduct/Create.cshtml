    <script>

     //InvestMent Product Lookup starts here
            $('#BenefactorCustomerAccountsGrid').DataTable({
            "sPaginationType": "full_numbers",
            "bProcessing": true,
            "bServerSide": true,
            "sAjaxSource": "@(Url.Action("Index", "InvestmentProduct", new { Area = "Accounts" }))",
            "sServerMethod": "POST",
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
                var createdDate = new Date(parseInt(aData.CreatedDate.replace("/Date(", "").replace(")/", ""), 10));
                var h = ("0" + createdDate.getHours()).slice(-2);
                var m = ("0" + createdDate.getMinutes()).slice(-2);
                var s = ("0" + createdDate.getSeconds()).slice(-2);

                var dt_Created = $.datepicker.formatDate('dd/mm/yy', createdDate) + " " + h + ":" + m + ":" + s;
                aData.CreatedDate = dt_Created;

                var selectButton = '<a href="#" data-id="' + aData.Id + '" class="btn btn-outline-info select-link">Select</a>';

                $('td:eq(3)', nRow).html(dt_Created);
                $('td:eq(4)', nRow).html(selectButton);

                return nRow;
            },
            "aoColumns": [
                { "mDataProp": "Description", "sTitle": "Name", "sWidth": "15%" },
                { "mDataProp": "PaddedCode", "sTitle": "Code", "sWidth": "10%" },
                { "mDataProp": "Priority", "sTitle": "Priority", "sWidth": "15%" },
                { "mDataProp": "CreatedDate", "sTitle": "Created Date", "sWidth": "10%" },
                { "mDataProp": "CreatedDate", "sTitle": "Action", "sWidth": "6%", "bSortable": false }
            ],
            "aaSorting": [[0, "desc"]]
        });

        // Handle the click event for the select link
                $(document).on('click', '.select-link', function (event) {
            event.preventDefault();
            var id = $(this).data('id');
            $.ajax({
                url: '@Url.Action("GetInvestmentProductDetails", "LoanProduct")',
                type: 'POST',
                data: { id: id },
                success: function (data) {
                    console.log(data); // Log the response data
                    if (data.success) {
                        $('#CustomerAccountTypeTargetProductDescription').val(data.data.CustomerAccountTypeTargetProductDescription);
                        $('#CustomerAccountTypeTargetProductId').val(data.data.CustomerAccountTypeTargetProductId);
                        $('#BenefactorcustomerLookupModal').modal('hide');
                    } else {
                        alert(data.message || 'Error retrieving data');
                    }
                },
                error: function (xhr, status, error) {
                    console.error("Error selecting debit type:", error);
                }
            });
        });
    </script>

    
