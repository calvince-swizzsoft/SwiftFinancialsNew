            function submitCyclesForm() {
                const minAmount = parseFloat(document.querySelector('[name="LoanRegistrationMinimumAmount"]').value);
                const maxAmount = parseFloat(document.querySelector('[name="LoanRegistrationMaximumAmount"]').value);
                const rangeLowerLimit = parseFloat(document.querySelector('[name="Cycles[0].RangeLowerLimit"]').value);
                const numCycles = 4; // Define the number of cycles

                // Calculate range per cycle
                const rangePerCycle = (maxAmount - minAmount) / numCycles;

                // Determine the upper limit for the current cycle based on the lower limit
                const rangeUpperLimit = rangeLowerLimit + rangePerCycle;

                // Check if the upper limit exceeds the max amount
                if (rangeUpperLimit > maxAmount) {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error!',
                        text: 'Upper limit exceeds maximum allowed amount.',
                        timer: 5000,
                        showConfirmButton: true
                    });
                    return;
