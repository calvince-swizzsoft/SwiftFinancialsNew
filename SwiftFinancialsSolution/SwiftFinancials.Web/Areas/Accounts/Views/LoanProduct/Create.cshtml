                // Prepare data for AJAX
                const formData = {
                    RangeLowerLimit: rangeLowerLimit,
                    RangeUpperLimit: rangeUpperLimit
                };

                $.ajax({
                    url: '/LoanProduct/AddCycles',
                    type: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify(formData),
                    success: function (response) {
                        if (response.success) {
