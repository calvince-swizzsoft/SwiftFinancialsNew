    <script>
        //Final Form Submission
        //Onclick Submit Button
        document.addEventListener('DOMContentLoaded', function () {
            const form = document.getElementById('form');
            const submitButton = document.getElementById('submitFormButton');
            if (!form || !submitButton) {
                console.error('Form or submit button not found');
                return;
            }
            submitButton.addEventListener('click', function (event) {
                event.preventDefault();
                const formData = new FormData(form);
                fetch('/LoanProduct/Create', {
                    method: 'POST',
                    body: formData,
                    headers: {
                        'X-Requested-With': 'XMLHttpRequest'
                    }
                })
                    .then(response => {
                        if (!response.ok) {
                            throw new Error('Network response was not ok');
                        }
                        return response.json();
                    })
                    .then(data => {
                        if (data.success) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Success!',
                                text: data.message,
                                confirmButtonColor: '#3085d6'
                            }).then(() => {
                                window.location.href = '/Accounts/LoanProduct/Index'; // Redirect to index
                            });
                            form.reset();
                        } else {
                            throw new Error(data.message);
                        }
                    })
                    .catch(error => {
                        console.error('Error:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Submission Failed',
                            text: error.message || 'There was an error creating the loan product. Please try again.',
                            confirmButtonColor: '#d33'
                        });
                    });
            });
        });
    </script>

    <script>
        $(document).ready(function () {
    // Function to collect selected loan IDs from each section
    function collectSelectedIds() {
        var selectedLoans = [];
        var selectedSavings = [];
        var selectedInvestments = [];

        // Collect selected Loans
        $('#LoanProductsGrid .lrowCheckbox:checked').each(function () {
            selectedLoans.push($(this).data('id'));
        });

        // Collect selected Savings
        $('#SavingsProductGrid .srowCheckbox:checked').each(function () {
            selectedSavings.push($(this).data('id'));
        });

        // Collect selected Investments
        $('#InvestmentProductGrid .irowCheckbox:checked').each(function () {
            selectedInvestments.push($(this).data('id'));
        });

        return {
            loans: selectedLoans,
            savings: selectedSavings,
            investments: selectedInvestments
        };
    }

    // Submit button for sending selected products to the server
    $('#submitButton').on('click', function () {
        var selectedIds = collectSelectedIds();

        $.ajax({
            url: '@Url.Action("ProcessSelectedProducts", "LoanProduct", new { Area = "Accounts" })',
            type: 'POST',
            data: {
                selectedLoans: selectedIds.loans,
                selectedSavings: selectedIds.savings,
                selectedInvestments: selectedIds.investments
            },
            success: function (response) {
                alert('Products processed successfully.');
            },
            error: function (error) {
                alert('An error occurred.');
            }
        });
    });
});

    </script>



