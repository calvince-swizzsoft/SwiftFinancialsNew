    <script>
        document.addEventListener('DOMContentLoaded', function () {
            var dropdown = document.getElementById('chargeDropdown');
            var percentageSection = document.getElementById('percentageSection');
            var fixedAmountSection = document.getElementById('fixedAmountSection');

            dropdown.addEventListener('change', function () {
                if (dropdown.value === '0') {
                    percentageSection.style.display = 'block';
                    fixedAmountSection.style.display = 'none';
                }
                else if (dropdown.value === '1') {
                    percentageSection.style.display = 'block';
                    fixedAmountSection.style.display = 'none';
                }
                else if (dropdown.value === '2') {
                    fixedAmountSection.style.display = 'block';
                    percentageSection.style.display = 'none';
                }
                else if (dropdown.value === '3') {
                    fixedAmountSection.style.display = 'block';
                    percentageSection.style.display = 'none';
                }
                else {
                    percentageSection.style.display = 'none';
                    fixedAmountSection.style.display = 'none';
                }
            });
        });

        $(document).ready(function () {
            // Handle button click to submit the form with AJAX
            $("#addEntryBtn2").click(function (e) {
                e.preventDefault();
                submitDeductiblesForm();
            });

            function submitDeductiblesForm() {
                // Get form data with corrected field selectors
                const description = $('[name="Deductiles[0].Description"]').val();
                const productCode = $('[name="Deductiles[0].CustomerAccountTypeProductCodeDescription"]').val();

                // Get the correct product description based on which section is visible
                let productDescription;
                if ($('#savingsProductSection').is(':visible')) {
                    productDescription = $('#CustomerAccountTypeTargetProductDescription1').val();
                } else if ($('#loansProductSection').is(':visible')) {
                    productDescription = $('#CustomerAccountTypeTargetProductDescription2').val();
                } else if ($('#investmentsProductSection').is(':visible')) {
                    productDescription = $('#CustomerAccountTypeTargetProductDescription').val();
                }

                const productId = $('#CustomerAccountTypeTargetProductId').val();
                const chargeType = $('[name="Deductiles[0].ChargeType"]').val();
                const chargePercentage = $('[name="Deductiles[0].ChargePercentage"]').val();
                const chargeFixedAmount = $('[name="Deductiles[0].ChargeFixedAmount"]').val();
                const computeChargeOnTopUp = $('[name="Deductiles[0].ComputeChargeOnTopUp"]').prop('checked');
                const netOffInvestmentBalance = $('[name="Deductiles[0].NetOffInvestmentBalance"]').prop('checked');

                const formData = {
                    Description: description,
                    CustomerAccountTypeProductCodeDescription: productCode,
                    CustomerAccountTypeTargetProductId: productId,
                    CustomerAccountTypeTargetProductDescription: productDescription,
                    ChargeType: chargeType,
                    ChargePercentage: chargePercentage,
                    ChargeFixedAmount: chargeFixedAmount,
                    ComputeChargeOnTopUp: computeChargeOnTopUp,
                    NetOffInvestmentBalance: netOffInvestmentBalance
                };

                $.ajax({
                    url: '/LoanProduct/AddDeductibles',
                    type: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify(formData),
                    success: function (response) {
                        if (response.success) {
                            updateEntriesTable(response.data);
                            Swal.fire({
                                icon: 'success',
                                title: 'Success!',
                                text: 'Deductible added successfully.',
                                timer: 1000,
                                showConfirmButton: false
                            });
                            clearFormFields();
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error!',
                                text: response.message || 'An error occurred while adding the deductible.',
                                timer: 8000,
                                showConfirmButton: true
                            });
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error("An error occurred:", error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error!',
                            text: 'An error occurred while processing your request.',
                            timer: 8000,
                            showConfirmButton: true
                        });
                    }
                });
            }

            function clearFormFields() {
                $('[name="Deductiles[0].Description"]').val('');
                $('[name="Deductiles[0].CustomerAccountTypeProductCodeDescription"]').val('');
                $('[name="Deductiles[0].ChargeType"]').val('');
                $('#CustomerAccountTypeTargetProductDescription').val('');
                $('#CustomerAccountTypeTargetProductDescription1').val('');
                $('#CustomerAccountTypeTargetProductDescription2').val('');
                $('#CustomerAccountTypeTargetProductId').val('');
                $('[name="Deductiles[0].ChargePercentage"]').val('');
                $('[name="Deductiles[0].ChargeFixedAmount"]').val('');
                $('[name="Deductiles[0].ComputeChargeOnTopUp"]').prop('checked', false);
                $('[name="Deductiles[0].NetOffInvestmentBalance"]').prop('checked', false);
            }

            function updateEntriesTable(entries) {
                var tableBody = $("#DeductilesTable tbody");
                tableBody.empty();

                $.each(entries, function (index, entry) {
                    var row = `
                                            <tr data-id="${entry.Id}">
                                                <td>${entry.Description || ''}</td>
                                                <td>${entry.CustomerAccountTypeTargetProductDescription || ''}</td>
                                                <td>${entry.ChargeType || ''}</td>
                                                <td>${entry.ChargeFixedAmount || ''}</td>
                                                <td>${entry.ChargePercentage || ''}</td>
                                                <td>${entry.NetOffInvestmentBalance ? 'Yes' : 'No'}</td>
                                                <td>${entry.ComputeChargeOnTopUp ? 'Yes' : 'No'}</td>
                                                <td>
                                                    <button type="button" class="btn btn-danger remove-entry" data-id="${entry.Id}">
                                                        Remove
                                                    </button>
                                                </td>
                                            </tr>`;
                    tableBody.append(row);
                });
            }

            // Handle remove entry
            $(document).on("click", ".remove-entry", function (event) {
                event.preventDefault();
                var entryId = $(this).data("id");
                removeEntry(entryId);
            });

            function removeEntry(id) {
                $.ajax({
                    url: '/LoanProduct/RemoveDeductibles',
                    type: 'POST',
                    data: { id: id },
                    success: function (response) {
                        if (response.success) {
                            updateEntriesTable(response.data);
                            Swal.fire({
                                icon: 'success',
                                title: 'Success!',
                                text: 'Entry removed successfully.',
                                timer: 1000,
                                showConfirmButton: false
                            });
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error!',
                                text: 'An error occurred while removing the entry.',
                                timer: 3000,
                                showConfirmButton: true
                            });
                        }
                    },
                    error: function () {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error!',
                            text: 'An error occurred while processing the request.',
                            timer: 3000,
                            showConfirmButton: true
                        });
                    }
                });
            }
        });
    </script>


    <script>
