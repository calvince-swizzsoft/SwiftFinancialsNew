        border: 1px solid black;
        border-radius: 12px;
        padding: 20px;
        margin-bottom: 20px;
        position: relative; /* Required for absolute positioning of h2 */
    }

    /* Styling for TierDetails div */
    .TierDetails {
        border: 1px solid black;
        border-radius: 12px;
        padding: 20px;
        margin-bottom: 5px;
        position: relative; /* Required for absolute positioning of h2 */
    }

    /* Styling for Tiers div */
