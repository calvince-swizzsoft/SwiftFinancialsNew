            TypeDescription: document.querySelector('[name="TypeDescription"]').value,
            Priority: document.querySelector('[name="Priority"]').value


        };

        // Send the data to the first controller using fetch
        fetch('/BatchOrigination_WireTransfer/BatchOrigination_WireTransfer', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'RequestVerificationToken': document.querySelector('[name="__RequestVerificationToken"]').value // Include this line if you are using Anti-Forgery Token
            },
            body: JSON.stringify(formData)
        })
            .then(response => response.json())
            .then(data => {
                // Handle the response data here
                console.log('Form Data Submitted:', data);
            })
            .catch((error) => {
                console.error('Error:', error);
            });
    }



