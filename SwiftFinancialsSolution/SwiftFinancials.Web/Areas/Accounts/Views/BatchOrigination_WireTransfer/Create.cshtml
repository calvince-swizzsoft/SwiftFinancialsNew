        background-color: white;
        color: #333;
        font-family: Arial, Helvetica, sans-serif;
    }

    #ChargeSplits th, #ChargeSplits td {
        padding: 12px;
        text-align: left;
        border: 1px solid #ddd;
    }

    #ChargeSplits th {
        font-weight: bold;
    }

    #ChargeSplits tbody tr:nth-child(even) {
        background-color: #f9f9f9;
    }

    #ChargeSplits tbody tr:hover {
        background-color: #f1f1f1;
    }

    .main1 {
        border: 1px solid black;
        border-radius: 12px;
        
        background-color: #f1f1f1;
    }


    .combined-container {
        padding: 10px; /* Padding inside the container */
        border-radius: 12px; /* Rounded corners (optional) */
        margin: 10px; /* Margin above and below the container */
        font-size: 15px; /* Set font size to 15px */
        display: flex; /* Enable Flexbox for alignment */
        flex-direction: column; /* Stack rows vertically */
        font-weight: bold;
        border: 4px solid #00ffff;
    }

    @*footer Styling*@

    .bottom-button {
        border: 2px solid gray; /* Border color set to gray */
        padding: 10px; /* Padding inside the border */
        border-radius: 5px; /* Rounded corners (optional) */

        margin: 10px;
        font-size: 18px;
        font-weight: bold;
        background-color: #00e0ff;
    }


    @*Styling for Entries*@
    .Wire-Transfer-container {
        border: 2px solid #333;
        margin: 20px;
        padding: 20px;
        font-size: 15px;
        border-radius: 8px;
        position: relative;
        padding: 40px; /* Space for the h2 */
        margin-bottom: 30px;
    }

        .Wire-Transfer-container h2 {
            position: absolute;
            top: -20px;
            left: 20px;
            background: #fff;
            padding: 0 10px;
            margin: 0;
        }
    input.form-control {
        color: #007bff; /* Replace with your desired color */
    }

    .form-control {
        color: #007bff;
    }

    .control-label {
        color: #333;
        font-weight:bold;
    }
    h2 {
        color: #333;
        font-weight:bold;
    }
</style>

