<script>

    function sendFormData() {
        // Create an object to hold the form data
        let formData = {
            Reference: document.querySelector('[name="Reference"]').value,
            TotalValue: document.querySelector('[name="TotalValue"]').value,
            PriorityDescription: document.querySelector('[name="PriorityDescription"]').value,
            WireTransferTypeDescription: document.querySelector('[name="WireTransferTypeDescription"]').value,
