        return $.getJSON(
         '@Url.Action("GetSavingsProductsAsync", "SavingsProduct", new { Area = "Accounts" })',
            { query: query },
            function (SavingProducts) {
                var newSavingProductsData = [];
                SavingProductsMap = {};
