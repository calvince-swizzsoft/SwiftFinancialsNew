    <script>
        $(document).ready(function () {
            // Function to handle tab switching and data fetching
            function handleTabClick(target) {
                if (target) {
                    // Switch active tabs
                    $('.inner-tab-pane').removeClass('active');
                    $(target).addClass('active');
                    $('.inner-nav-link').removeClass('active');
                    $('a[href="' + target + '"]').addClass('active');

                    // Derive transaction type from target ID
                    var transactionType = '';
                    switch (target) {
                        case '#cashDeposit':
                            transactionType = 'Cash Deposit';
                            break;
                        case '#cashWithdrawal':
                            transactionType = 'Cash Withdrawal';
                            break;
                        case '#chequeDeposit':
                            transactionType = 'Cheque Deposit';
                            break;
                        default:
                            transactionType = 'Unknown Transaction';
                    }

                    // Call the FetchKnownCharges method via AJAX
                    $.ajax({
                        url: '/CashDeposit/FetchKnownCharges',
                        type: 'POST',
                        data: { target: target },
                        success: function (response) {
                            if (response.success) {
                                var model = response.data;
                                var commissions = model.Commissions;


                                if (commissions && commissions.length > 0) {
                                    var tableBody = '';
                                    $.each(commissions, function (index, commission) {
                                        tableBody += '<tr>' +
                                            '<td>' + commission.Description + '</td>' +
                                            '<td>' + commission.MaximumCharge + '</td>' +
                                            '<td>' + commission.ExtendedChargeBenefactorDescription + '</td>' +
                                            '</tr>';
                                    });

                                    // Insert the rows into the tbody of the table
                                    $('#chargesLevies tbody').html(tableBody);
                                } else {
                                    // If no commissions, display a message with the transaction type
                                    var noDataMessage = 'No commissions available for this ' + transactionType + ' transaction';
                                    $('#chargesLevies tbody').html('<tr><td colspan="3">' + noDataMessage + '</td></tr>');
                                }
                            } else {
                                console.log("Error:", response.message);
                            }
                        },
                        error: function (xhr, status, error) {
                            console.log("AJAX error:", error);
                        }
                    });
                }
            }

            // Bind click events to the inner-nav-link elements
            $('.inner-nav-link').on('click', function (e) {
                e.preventDefault();
                var target = $(this).attr('href');
                handleTabClick(target);
            });

            // Trigger the default tab (e.g., #cashDeposit) on page load
            handleTabClick('#cashDeposit');
        });
    </script>





