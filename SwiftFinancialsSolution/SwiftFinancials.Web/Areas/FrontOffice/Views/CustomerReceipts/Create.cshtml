<script>
    $(document).ready(function () {
        $('.nav-link').on('click', function (e) {
            e.preventDefault();
            var target = $(this).attr('href');

            // Switch tab content
            $('.tab-pane').removeClass('active');
            $(target).addClass('active');

            // Update tab styles
            $('.nav-link').removeClass('active').css('background-color', '#f1f1f1');
            $(this).addClass('active').css('background-color', '#ddd');

            // Update tab content area background
            $('.tab-content').css('background-color', '#ddd');
        });

        $('.secondary-nav-link').on('click', function (e) {
            e.preventDefault();
            var target = $(this).attr('href');

            // Switch secondary tab content
            $('.secondary-tab-pane').removeClass('active');
            $(target).addClass('active');

            // Update secondary tab styles
            $('.secondary-nav-link').removeClass('active');
            $(this).addClass('active');
        });
    });
</script>
