@model  Application.MainBoundedContext.DTO.FrontOfficeModule.FiscalCountDTO

@{
    /**/

    ViewBag.Title = "Create CashManagement";

    var withdrawalNotificationCategories = ViewBag.WithdrawalNotificationCategorySelectList as List<SelectListItem>;

    var treasuryTransactionTypes = ViewBag.TreasuryTransactionTypeSelectList as List<SelectListItem>;

}

@using (Html.BeginForm("Search", "CashManagement", new { Area = "FrontOffice" }, FormMethod.Get))
{
    TempData["Remarks"] = "HAM";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <h2>Cash Management</h2>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                @Html.ActionLink("Dashboard", "Index", "Home", new { Area = "" }, null)
            </li>
            <li class="breadcrumb-item">
                @Html.ActionLink("Home", "Index", "CashManagement", new { Area = "FrontOffice" }, null)
            </li>
            <li class="breadcrumb-item active">
                <strong>Create</strong>
            </li>
        </ol>
    </div>
</div>

@if (TempData["Error"] != null)
{
    <div class="alert alert-danger fade in">
        <button class="close" data-dismiss="alert">
            ×
        </button>
        <i class="fa-fw fa fa-times"></i>
        <strong>Error!</strong> @TempData["Error"]
    </div>
}
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content bg-white text-info">
                    @using (Html.BeginForm("Create", "CashManagement", new { Area = "FrontOffice" }, FormMethod.Post))
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })


                            <fieldset>
                                <div class="row">
                                    <div class="col-lg-6">


                                        <div class="form-group form-group-lg">
                                            @Html.LabelFor(model => model.TransactionType, new { @class = "control-label" })
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bars fa-fw"></i></span>
                                                @Html.DropDownListFor(model => model.TransactionType, treasuryTransactionTypes, new { @class = "form-control input-md", id = "TransactionType" })

                                                @Html.ValidationMessageFor(model => model.TransactionType)
                                            </div>
                                        </div>


                                        <div class="form-group form-group-sm">
                                            @Html.LabelFor(model => model.TotalAmount, htmlAttributes: new { @class = "control-label" })
                                            @Html.EditorFor(model => model.TotalAmount, new { htmlAttributes = new { @id = "Remark", @class = "form-control", @placeholder = "Amount", @required = "" } })
                                            @Html.ValidationMessageFor(model => model.TotalAmount, "", new { @class = "text-danger" })
                                        </div>








                                        <div class="form-group form-group-sm">
                                            <div class="col-md-offset-6">
                                                <input type="submit" value="Create" class="btn btn-primary" />
                                            </div>
                                        </div>



                                    </div>


                                    <div class="col-lg-6">


                                        <div class="form-group form-group-sm">
                                            @Html.LabelFor(model => model.Description, htmlAttributes: new { @class = "control-label" })
                                            @Html.EditorFor(model => model.Description, new { htmlAttributes = new { @id = "Description", @class = "form-control", @placeholder = "Description", disabled = "disabled" } })
                                            @Html.HiddenFor(model => model.Description)                                        
                                            @Html.ValidationMessageFor(model => model.Description, "", new { @class = "text-danger" })
                                        </div>


                                        <div class="form-group form-group-sm">
                                            @Html.LabelFor(model => model.Reference, htmlAttributes: new { @class = "control-label" })
                                            @Html.EditorFor(model => model.Reference, new { htmlAttributes = new { @id = "Remark", @class = "form-control", @placeholder = "Remarks", @required = "" } })
                                            @Html.ValidationMessageFor(model => model.Reference, "", new { @class = "text-danger" })
                                        </div>





                                    </div>
                                </div>

                            </fieldset>

                        </div>
                    }
                </div>

            </div>

        </div>
    </div>
</div>









