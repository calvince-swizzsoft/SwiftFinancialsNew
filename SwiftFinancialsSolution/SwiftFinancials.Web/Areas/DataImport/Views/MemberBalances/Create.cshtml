

@{
    ViewBag.Title = "Create";
}

<h2>Import Member Balances</h2>

@using (Html.BeginForm("Create", "MemberBalances", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-group">
        <label for="uploadedFile">Select Excel File</label>
        <input type="file" id="uploadedFile" name="uploadedFile" class="form-control" />
    </div>
    <div class="form-group mt-3">
        <button type="submit" class="btn btn-primary">Upload</button>
    </div>
}

